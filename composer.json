{
    "name": "presseddigital/validateit",
    "description": "Supercharged text field validation.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craftcms",
        "validation",
    ],
    "support": {
        "docs": "https://github.com/presseddigital/validateit/blob/master/README.md",
        "issues": "https://github.com/presseddigital/validateit/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Pressed Digital",
            "homepage": "https://pressed.digital"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.2"
    },
    "autoload": {
        "psr-4": {
          "presseddigital\\validateit\\": "src/"
        }
    },
    "extra": {
        "name": "Validateit",
        "handle": "validateit",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/presseddigital/validateit/master/CHANGELOG.md",
        "class": "presseddigital\\validateit\\Validateit"
    }
}
